export interface infoInterface {
  {{#each _info}}
  {{@key}}: 
  {{#if (isObj this) }}
  {
    {{#each this}}
      {{@key}}: string;
    {{/each}}
  },
  {{else}}string;
  {{/if}}
  {{/each}}
}

export const info: infoInterface = {
  {{#each _info}}
  {{@key}}: 
  {{#if (isObj this) }}
  {
    {{#each this}}
        {{@key}}: "{{this}}",
    {{/each}}
  },
  {{else}}"{{this}}",
  {{/if}}
  {{/each}}
};

