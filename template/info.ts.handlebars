export interface InfoInterface {
  title: string;
  description?: string;
  termsOfService?: string;
  contact?: {
    name?: string;
    url?: string;
    email?: string;
  };
  license?: {
    name: string;
    url?: string;
  };
  version: string;
}

export const Info: InfoInterface = {
  {{#each _info}}
  {{@key}}: 
  {{#if (isObj this) }}
  {
    {{#each this}}
        {{@key}}: "{{this}}",
    {{/each}}
  },
  {{else}}"{{this}}",
  {{/if}}
  {{/each}}
};

